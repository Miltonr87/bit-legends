{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; script-src 'self' https://www.retrogames.cc; frame-src 'self' https://www.retrogames.cc; img-src 'self' data: https://www.retrogames.cc; style-src 'self' 'unsafe-inline'; object-src 'none'; connect-src 'self'; media-src 'self' https://www.retrogames.cc; font-src 'self';"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}